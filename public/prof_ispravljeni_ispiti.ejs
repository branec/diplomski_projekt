    <div class="table-content">
        <h2>Popis ispravljenih ispita:</h2>
        <!--<select class="izbornik-predmeti">
            <option>Svi ispiti</option>
            <option>Diplomski projekt</option>
            <option>MIS</option>
        </select>-->
        <table class="table">
           
                    <th></th>
                    <th>Student</th>
                    <th>Predmet</th>
                    <th>Ispit</th>
                    <th>Bodovi</th>
                    <th></th>
                    <th></th>
            
                    <%for (i=0; i<predmeti.length; i++){%>
                        <tr>
                            <td><%= i+1 %>.</td>
                            <td> <%= predmeti[i].korisnik %></td>
                            <td><%= predmeti[i].nazivPredmeta %></td>
                            <td><%= predmeti[i].nazivIspita %></td>
                            <td><%= predmeti[i].bodovi %> /  <%= predmeti[i].max %></td>
                            <td><a >Korigiraj</a></td>
                        </tr>
                    <% } %>

           
        </table>

    <!-- /#wrapper -->

