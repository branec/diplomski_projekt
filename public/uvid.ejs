<!DOCTYPE html>
<head>
	
	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js"></script>

	<script type="text/javascript" src="scripts/fancybox/jquery.mousewheel-3.0.4.pack.js"></script>
	<script type="text/javascript" src="scripts/fancybox/jquery.fancybox-1.3.4.pack.js"></script>
	<link rel="stylesheet" type="text/css" href="scripts/fancybox/jquery.fancybox-1.3.4.css" media="screen" />
     <link rel="stylesheet" href="scripts/styles.css" />
     <link href="/scripts/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
     <link href="/scripts/sidebar.css" rel="stylesheet">
     <link href="/scripts/styles.css" rel="stylesheet">
	<script type="text/javascript">
		$(document).ready(function() {
			$("a[rel=example_group]").fancybox();
		});
	</script>
</head>

<body onload='Draw()'>
        <header>
                <div class="header-middle">
                <ul class="header-navigation">
                    <li><img src="/images/fer_logo.PNG" /></li>
                    <li><a id="header-scanner" >Dodaj novi ispit</a></li>
                    <li>|</li>
                    <li><a  id="header-ispiti">Pregled rezultata</a></li>
                    <li>|</li>
                    <li><a href="/logout">Odjava</a></li>
                </ul>
                </div>
            </header>
            <div id="wrapper">
                    <div id="sidebar-wrapper">
                        <ul class="sidebar-nav">
                         
                            <li>
                                <a href="/scanner">Dodaj novi ispit</a>
                            </li>
                            <li>
                                <a href="/ispiti">Pregled rezultata</a>
                            </li>
                            <li>
                                <a href="/logout">Odjava</a>
                            </li>
                        </ul>
                    </div>
            
                    <div id="page-content-wrapper">
                        <div class="container-fluid">
                            <a href="#menu-toggle" class="btn btn-secondary" id="menu-toggle"><img width="30px" src="images/ham-meni.png" /></a>
                        </div>
                    </div>
              
            
                </div>

<h2 style="margin-left:10%;margin-top:5%;">Kolokvij iz predmeta Raƒçunalna forenzika</h2>
<div id="content">
	<p>
    <% for (i=0; i < slike.length; i++) {%>
         <a rel="example_group" href="<% slike[i].url %>" title=""><img width="250px" alt="" src="<% slike[i].url %>" /></a>
  <% } %>
    </p>
</div>
<div class="exam-details">
    <div class="first-section">
            <p>Ispravio: <%= kosuljica.Ime + ' ' + kosuljica.Prezime %></p>
            <p>Dvorana: <%= kosuljica.Prostorija %></p>
            <P>Vrijeme predaje ispita: <%= kosuljica.VrijemeDo %></p>
            <p>Bodovi: <%= ukupnoBodova %></p>
    </div>
    <div class="second-section">
        <% for (i=0; i < bodovi.length; i++) {%>
            <p><%= bodovi[i].Zadatak %> zadatak: <%= bodovi[i].Bodovi %> bodova</p>
        <% } %>
    </div>
   
</div>

<script src="/scripts/vendor/jquery/jquery.min.js"></script>
<script src="/scripts/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="scripts/headerStudent.js"></script>
<script src="scripts/newSheet.js"></script>

<!-- Menu Toggle Script -->
<script>
$("#menu-toggle").click(function(e) {
    e.preventDefault();
    $("#wrapper").toggleClass("toggled");
    $("#menu-toggle").addClass("opened-hamburger");
});
</script>
</body>
</html>